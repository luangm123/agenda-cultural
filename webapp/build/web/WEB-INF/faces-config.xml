<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <application>
        <el-resolver>jsf.util.JsfCrudELResolver</el-resolver>
    </application>
    <navigation-rule>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>eventos</managed-bean-name>
        <managed-bean-class>jsf.EventosController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>eventosJpa</managed-bean-name>
        <managed-bean-class>jpa.EventosJpaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>bd.Eventos</converter-for-class>
        <converter-class>jsf.EventosConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>eventos_create</from-outcome>
            <to-view-id>/eventos/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>eventos_list</from-outcome>
            <to-view-id>/eventos/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>eventos_edit</from-outcome>
            <to-view-id>/eventos/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>eventos_detail</from-outcome>
            <to-view-id>/eventos/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>locais</managed-bean-name>
        <managed-bean-class>jsf.LocaisController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>locaisJpa</managed-bean-name>
        <managed-bean-class>jpa.LocaisJpaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>bd.Locais</converter-for-class>
        <converter-class>jsf.LocaisConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>locais_create</from-outcome>
            <to-view-id>/locais/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>locais_list</from-outcome>
            <to-view-id>/locais/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>locais_edit</from-outcome>
            <to-view-id>/locais/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>locais_detail</from-outcome>
            <to-view-id>/locais/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>tipoEventos</managed-bean-name>
        <managed-bean-class>jsf.TipoEventosController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>tipoEventosJpa</managed-bean-name>
        <managed-bean-class>jpa.TipoEventosJpaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>bd.TipoEventos</converter-for-class>
        <converter-class>jsf.TipoEventosConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>tipoEventos_create</from-outcome>
            <to-view-id>/tipoEventos/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>tipoEventos_list</from-outcome>
            <to-view-id>/tipoEventos/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>tipoEventos_edit</from-outcome>
            <to-view-id>/tipoEventos/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>tipoEventos_detail</from-outcome>
            <to-view-id>/tipoEventos/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
